@article{ ISI:000358147500053,
Author = {Yin, Guodong and Zhang, Yuan and Sun, Yan and Wang, Tao and Zeng,
   Zhenzhong and Piao, Shilong},
Title = {{MODIS Based Estimation of Forest Aboveground Biomass in China}},
Journal = {{PLOS ONE}},
Year = {{2015}},
