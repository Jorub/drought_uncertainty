@article{ ISI:000287741300002,
Author = {Grenouillet, Gael and Buisson, Laetitia and Casajus, Nicolas and Lek,
   Sovan},
Title = {{Ensemble modelling of species distribution: the effects of geographical
   and environmental ranges}},
