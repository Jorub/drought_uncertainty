@inproceedings{ ISI:000324148900001,
Author = {Strasser, U. and Kunstmann, H.},
Editor = {{Gelfan, A and Yang, D and Gusev, Y and Kunstmann, H}},
Title = {{Tackling complexity in modelling mountain hydrology: where do we stand,
   where do we go?}},
Booktitle = {{COLD AND MOUNTAIN REGION HYDROLOGICAL SYSTEMS UNDER CLIMATE CHANGE:
   TOWARDS IMPROVED PROJECTIONS}},
