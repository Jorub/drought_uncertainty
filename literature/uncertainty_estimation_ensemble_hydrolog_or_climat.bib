@article{ ISI:000238689800013,
Author = {Slater, Andrew G. and Clark, Martyn P.},
Title = {{Snow data assimilation via an ensemble Kalman filter}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2006}},
