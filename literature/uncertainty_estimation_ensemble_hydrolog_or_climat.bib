@article{ ISI:000267202000010,
Author = {Orescanin, Biljana and Rajkovic, Borivoj and Zupanski, Milija and
   Zupanski, Dusanka},
Title = {{Soil model parameter estimation with ensemble data assimilation}},
Journal = {{ATMOSPHERIC SCIENCE LETTERS}},
Year = {{2009}},
Volume = {{10}},
Number = {{2}},
Pages = {{127-131}},
Month = {{APR}},
Abstract = {{A parameter estimation problem in context of ensemble data assimilation
   is addressed. In an example using a one-point soil temperature model,
   the parameters corresponding to the emissivity and to the effective
   depth between the surface and the lowest atmospheric model level are
   estimated together with the initial conditions for temperature. The
   nonlinear synthetic observations representing various fluxes are
   assimilated using the Maximum Likelihood Ensemble Filter (MLEF). The
   results indicate a benefit of simultaneous assimilation of initial
   conditions and parameters. The estimated uncertainties are in general
   agreement with actual uncertainties. Copyright (C) 2009 Royal
   Meteorological Society}},
