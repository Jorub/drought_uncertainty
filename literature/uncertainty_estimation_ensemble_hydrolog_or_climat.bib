@article{ ISI:000382112000030,
Author = {De La Chevrotiere, Michele and Khouider, Boualem and Majda, Andrew J.},
Title = {{Stochasticity of convection in Giga-LES data}},
Journal = {{CLIMATE DYNAMICS}},
