@article{ ISI:000363532600003,
Author = {Lee, Han Soo and Kaneko, Arata},
Title = {{Estimation and Projection of Non-Linear Relative Sea-Level Rise in the
   Seto Inland Sea, Japan}},
Journal = {{ATMOSPHERE-OCEAN}},
