@article{ ISI:000189200600001,
Author = {Benestad, RE},
Title = {{Tentative probabilistic temperature scenarios for northern Europe}},
Journal = {{TELLUS SERIES A-DYNAMIC METEOROLOGY AND OCEANOGRAPHY}},
