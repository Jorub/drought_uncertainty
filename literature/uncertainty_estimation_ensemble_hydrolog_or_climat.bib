@article{ ISI:000321480400004,
Author = {Steffens, Karin and Larsbo, Mats and Moeys, Julien and Jarvis, Nicholas
   and Lewan, Elisabet},
Title = {{Predicting pesticide leaching under climate change: Importance of model
   structure and parameter uncertainty}},
Journal = {{AGRICULTURE ECOSYSTEMS \& ENVIRONMENT}},
