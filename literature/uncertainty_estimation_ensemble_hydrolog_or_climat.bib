@article{ ISI:000294352100001,
Author = {Eliseev, A. V.},
Title = {{Estimation of Changes in Characteristics of the Climate and Carbon Cycle
   in the 21st Century Accounting for the Uncertainty of Terrestrial Biota
   Parameter Values}},
