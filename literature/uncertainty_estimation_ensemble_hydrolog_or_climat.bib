@article{ ISI:000405926700013,
Author = {Wong, Tony E. and Kleiber, William and Noone, David C.},
Title = {{The Impact of Error Accounting in a Bayesian Approach to Calibrating
   Modeled Turbulent Fluxes in an Open-Canopy Forest}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
