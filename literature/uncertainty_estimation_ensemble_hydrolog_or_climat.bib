@article{ ISI:000376408600008,
Author = {Gao, Chuan and Wu, Xinrong and Zhang, Rong-Hua},
Title = {{Testing a four-dimensional variational data assimilation method using an
   improved intermediate coupled model for ENSO analysis and prediction}},
Journal = {{ADVANCES IN ATMOSPHERIC SCIENCES}},
Year = {{2016}},
Volume = {{33}},
Number = {{7}},
Pages = {{875-888}},
Month = {{JUL}},
Abstract = {{A four-dimensional variational (4D-Var) data assimilation method is
   implemented in an improved intermediate coupled model (ICM) of the
   tropical Pacific. A twin experiment is designed to evaluate the impact
   of the 4D-Var data assimilation algorithm on ENSO analysis and
   prediction based on the ICM. The model error is assumed to arise only
   from the parameter uncertainty. The ``observation{''} of the SST
   anomaly, which is sampled from a ``truth{''} model simulation that takes
   default parameter values and has Gaussian noise added, is directly
   assimilated into the assimilation model with its parameters set
   erroneously. Results show that 4D-Var effectively reduces the error of
   ENSO analysis and therefore improves the prediction skill of ENSO events
   compared with the non-assimilation case. These results provide a
   promising way for the ICM to achieve better real-time ENSO prediction.}},
