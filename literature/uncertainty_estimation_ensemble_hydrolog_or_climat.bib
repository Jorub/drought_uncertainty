@article{ ISI:000334153300005,
Author = {Klein, Bastian and Meissner, Dennis and Pinzinger, Robert and Lisniak,
   Dmytro},
Title = {{Estimation of the predictive uncertainty of hydrological model
   simulations and forecasts using copulas}},
Journal = {{HYDROLOGIE UND WASSERBEWIRTSCHAFTUNG}},
Year = {{2014}},
Volume = {{58}},
Number = {{2}},
Pages = {{95-105}},
Month = {{APR}},
Abstract = {{A method to estimate the predictive uncertainty of hydrological model
   simulations and forecasts based on the model performance in the past is
   presented in this paper. Copulas are applied to estimate the probability
   distribution of the ``real{''} value of interest (e.g. observed runoff)
   conditional on the hydrological model simulation. The joint probability
   distribution of correlated random variables can be expressed in terms of
   the univariate marginal distributions and a copula function. This method
   is applied to hydrological model simulations at two gauges in the River
   Rhine Basin and the results are compared to existing methods to estimate
   the predictive uncertainty.}},
