@article{ ISI:000330203600007,
Author = {Villarini, Gabriele and Seo, Bong-Chul and Serinaldi, Francesco and
   Krajewski, Witold F.},
Title = {{Spatial and temporal modeling of radar rainfall uncertainties}},
